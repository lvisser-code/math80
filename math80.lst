     804                  ;   SUBROUTINE NORM
     805                  ;
     806                  ;       THIS SUBROUTINE WILL NORMALIZE A FLOATING POINT
     807                  ;       NUMBER, PRESERVING ITS ORIGINAL SIGN.
     808                  ;       WE CHECK FOR UNDERFLOW AND SET THE CONDITION
     809                  ;       FLAG APPROPRIATELY.  (SEE ERROR RETURNS).
     810                  ;       THERE IS AN ENTRY POINT TO FLOAT A SIGNED INTEGER
     811                  ;       (FLOAT) AND AN ENTRY POINT TO FLOAT AN UNSIGNED
     812                  ;       INTEGER.
     813                  ;
     814                  ;   ENTRY POINTS:
     815                  ;
     816                  ;       NORM  - NORMALIZE FLOATING PT NUMBER AT (H,L)
     817                  ;       FLOAT - FLOAT TRIPLE PRECISION INTEGER AT (H,L)
     818                  ;               PRESERVING SIGN BIT IN (H,L)+3
     819                  ;       DFXL  - FLOAT UNSIGNED (POSITIVE) TRIPLE PRECISION
     820                  ;               AT (H,L)
     821                  ;
     822                  ;   REGISTERS ON EXIT:
     823                  ;
     824                  ;       A = CONDITION FLAG (SEE ERROR RETURNS)
     825                  ;       D,E = GARBAGE
     826                  ;       B,C,H,L = SAME AS ON ENTRY
     827                  ;
     828  0431 5D         NORM:   MOV  E,L        ;SAVE L IN E
     829  0432 CD C5 04   NORM1:  CALL GCHAR      ;GET CHAR(H,L) IN A WITH SIGN EXTENDED
     830  0435 57                 MOV  D,A        ;SAVE CHAR IN D
     831  0436 6B         FXL1:   MOV  L,E        ;RESTORE L
     832  0437 CD 5E 04   FXL2:   CALL ZMCHK      ;CHECK FOR ZERO MANTISSA
     833  043A CA 11 05           JZ   WZER       ;IF ZERO MANTISSA THEN ZERO RESULT
     834  043D 7E         REP6:   MOV  A,M        ;GET MOST SIGNIFICANT BYTE OF
     835                  ;MANTISSA
     836  043E B7                 ORA  A          ;SET FLAGS
     837  043F FA 4F 04           JM   SCHAR      ;IF MOST SIGNFICANT BIT = 1 THEN
     838                  ;NUMBER IS NORMALIZED AND WE GO TO
     839                  ;STORE THE CHARACTERISTIC
     840  0442 7A                 MOV  A,D        ;OTHERWISE CHECK FOR UNDERFLOW
     841  0443 FE C0              CPI  MINCH      ;COMPARE WITH MINIMUM CHAR
     842  0445 CA E7 04           JZ   WUND       ;IF EQUAL THEN UNDERFLOW
     843  0448 CD 6D 04           CALL DLST       ;SHIFT MANTISSA LEFT
     844  044B 15                 DCR  D          ;DECREMENT CHARACTERSTIC
     845  044C C3 3D 04           JMP  REP6       ;LOOP AN TEST NEXT BIT
     846  044F C3 47 05   SCHAR:  JMP  INCR3      ;STORE THE CHARACTERISTIC USING
     847                  ;THE SAME CODE AS THE INCREMENT
     848                  ;
     849  0452 5D         DFXL:   MOV  E,L        ;ENTER HERE TO FLOAT UNSIGNED
     850                  ;INTEGER
     851                  ;FIRT SAVE L IN E
     852  0453 2C                 INR  L          ;MAKE (H,L) POINT TO CHAR
     853  0454 2C                 INR  L          ;MAKE (H,L) POINT TO CHAR
     854  0455 2C                 INR  L          ;MAKE (H,L) POINT TO CHAR
     855  0456 AF                 XRA  A          ;ZERO ACCUMULATOR
     856  0457 77                 MOV  M,A        ;STORE A PLUS (+) SIGN
     857  0458 6B                 MOV  L,E        ;RESTORE L
     858  0459 16 18      FLOAT:  MVI  D,24       ;ENTER HERE TO FLOAT INTEGER
     859                  ;PRESERVING ORIGINAL SIGN IN (H,L)+3
     860                  ;SET UP CHARACTERISTIC
     861  045B C3 37 04           JMP  FXL2       ;GO FLOAT THE NUMBER
     862                  ;
