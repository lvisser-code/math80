     597  02EC C8                 RZ              ;IF SO, RESULT=ARG SO RETURN
     598                  ;THIS WILL PREVENT UNDERFLOW INDICATION ON
     599                  ;ZERO + OR - ZERO
     600  02ED CD DF 04           CALL CCMP       
     601  02F0 CA 20 03           JZ   EQ02       ;IF EQUAL, GO ON
     602  02F3 57                 MOV  D,A        ;SAVE LPTR CHAR IN D
     603  02F4 DA 03 03           JC   LLTB       
