     309                  ;(HL, DE) 16 bit signed integer multiplication: HL * DE
     310                  ;Returns HL, CY = 1 if overflow
     311  018A C5         IMUL:   PUSH B          
     312  018B D5                 PUSH D          
     313  018C 01 08 00           LXI  B, 8       ;B = 0 (sign), C = 8 (loop counter)
     314  018F CD 3E 00           CALL IABS       ;Make both args positive while
     315  0192 D2 96 01           JNC  IMUL1      ;  saving their signs in B
     316  0195 04                 INR  B          
     317  0196 EB         IMUL1:  XCHG            
     318  0197 CD 3E 00           CALL IABS       
     319  019A D2 9E 01           JNC  IMUL2      
     320  019D 04                 INR  B          
     321  019E CD C2 00   IMUL2:  CALL ICMP       ;Compare HL&DE
     322  01A1 DA A5 01           JC   IMUL3      ;If HL is larger, then exchange
     323  01A4 EB                 XCHG            ;DE = multiplicand, HL = multiplier
     324  01A5 97         IMUL3:  SUB  A          ;H must be = 0, else overflow
     325  01A6 BC                 CMP  H          
     326  01A7 C2 C4 01           JNZ  IMULX      ;Exit with CY=1 if overflow
     327  01AA 7D                 MOV  A, L       ;Let A = multipler
     328  01AB 21 00 00           LXI  H, 0       ;Init product = 0
     329  01AE 29         IMUL4:  DAD  H          ;Rotate product left
     330  01AF 17                 RAL             ;Rotate multiplier left into CY
     331  01B0 D2 B7 01           JNC  IMUL5      
     332  01B3 19                 DAD  D          ;Add multiplicand to partial sum
     333  01B4 DA C4 01           JC   IMULX      ;Exit with CY=1 if overflow
     334  01B7 0D         IMUL5:  DCR  C          ;Next
     335  01B8 C2 AE 01           JNZ  IMUL4      
     336  01BB 78                 MOV  A, B       ;If sign of 1 arg was negative
     337  01BC 1F                 RAR             
     338  01BD D2 C4 01           JNC  IMULX      
     339  01C0 CD C7 01           CALL INEG       ;  then product is negative
     340  01C3 97                 SUB  A          ;CY=0
     341  01C4 D1         IMULX:  POP  D          
     342  01C5 C1                 POP  B          
     343  01C6 C9                 RET             
