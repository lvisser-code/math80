     100                  ;(HL, DE) 16 bit signed integer addition: HL + DE. Returns HL, CY = overflow
     101  0045 7C         IADD:   MOV  A, H       ;HL and DE both positive?
     102  0046 07                 RLC             
     103  0047 DA 53 00           JC   IADDN      
     104  004A 7A                 MOV  A, D       
     105  004B 07                 RLC             
     106  004C DA 53 00           JC   IADDN      
     107  004F 19                 DAD  D          ;  Yes, then add
     108  0050 7C                 MOV  A, H       
     109  0051 07                 RLC             
     110  0052 C9                 RET             
     111  0053 7C         IADDN:  MOV  A, H       ;HL and DE both negative?
     112  0054 07                 RLC             
     113  0055 D2 62 00           JNC  IADDM      
     114  0058 7A                 MOV  A, D       
     115  0059 07                 RLC             
     116  005A D2 62 00           JNC  IADDM      
     117  005D 19                 DAD  D          ;  Yes, then add
     118  005E 7C                 MOV  A, H       
     119  005F 07                 RLC             
     120  0060 3F                 CMC             
     121  0061 C9                 RET             
     122  0062 19         IADDM:  DAD  D          ;HL and DE have mixed signs
     123  0063 37                 STC             
     124  0064 3F                 CMC             
     125  0065 C9                 RET             
