    1476  05D9 57                 MOV  D,A        ;SAVE CHARACTERISTIC IN D
    1477  05DA CD 47 05           CALL INCR3      ;STORE CHARACTERISTIC
    1478  05DD 6B                 MOV  L,E        ;RESTORE L
    1479  05DE C9                 RET             ;RETURN
