     138                  ;(@DE) 16 bit signed convert ASCII Decimal number to Integer.
     139                  ;Returns HL=value, CY=err
     140  006D C5         IASC:   PUSH B          
     141  006E 21 00 00           LXI  H, 0       ;Init result = 0
     142  0071 97                 SUB  A          
     143  0072 32 01 00           STA  ISIGN      ;Init sign = 0 (positive)
     144  0075 CD 75 02           CALL NOSPC      ;Skip leading spaces, A=1st char
     145  0078 FE 2B              CPI  '+'        ;Skip leading '+'
     146  007A C2 82 00           JNZ  ASCIN0     
     147  007D 13                 INX  D          
     148  007E 1A                 LDAX D          
     149  007F C3 8E 00           JMP  ASCIN1     
     150  0082 FE 2D      ASCIN0: CPI  '-'        ;Negative?
     151  0084 C2 8E 00           JNZ  ASCIN1     
     152  0087 3E 01              MVI  A, 1       ;Sign = 1 (negative)
     153  0089 32 01 00           STA  ISIGN      
     154  008C 13                 INX  D          
     155  008D 1A                 LDAX D          
     156  008E CD 7D 02   ASCIN1: CALL IS09       ;Check for a digit
     157  0091 DA C0 00           JC   ASCIXT     ;  if not exit with CY=1
     158  0094 D6 30      ASCIN2: SUI  '0'        ;Convert ASCII digit to hex byte
     159  0096 44                 MOV  B, H       ;Mult HL by 10
     160  0097 4D                 MOV  C, L       
     161  0098 29                 DAD  H          ;  x2
     162  0099 29                 DAD  H          ;  x4
     163  009A 29                 DAD  H          ;  x8
     164  009B 09                 DAD  B          ;  x9
     165  009C 09                 DAD  B          ;  x10
     166  009D DA C0 00           JC   ASCIXT     ;Overflow (>FFFF)?
     167  00A0 06 00              MVI  B, 0       ;Add byte in A to HL
     168  00A2 4F                 MOV  C, A       
     169  00A3 09                 DAD  B          
     170  00A4 DA C0 00           JC   ASCIXT     ;Overflow (>FFFF)?
     171  00A7 13                 INX  D          ;A = next Char
     172  00A8 1A                 LDAX D          
     173  00A9 CD 7D 02           CALL IS09       ;Another digit?
     174  00AC D2 94 00           JNC  ASCIN2     ;  loop if yes
     175  00AF 7C                 MOV  A, H       ;Result > 7FFFH (Overflow)?
     176  00B0 E6 80              ANI  80H        
     177  00B2 C2 C0 00           JNZ  ASCIXT     
     178  00B5 3A 01 00           LDA  ISIGN      ;Check sign
     179  00B8 B7                 ORA  A          
     180  00B9 CA BF 00           JZ   ASCIN4     
     181  00BC CD C7 01           CALL INEG       ;Negative so form 2's complement
     182  00BF 97         ASCIN4: SUB  A          ;Successful conversion, CY=0
     183  00C0 C1         ASCIXT: POP  B          ;Exit
     184  00C1 C9                 RET             
