     630  031F 6E                 MOV  L,M        ;RESTORE L
     631  0320 CD 86 03   EQ02:   CALL LASD       ;CHECK WHAT TO
     632  0323 CD 73 05           CALL ACPR       ;SAVE ANSWER
     633  0326 FE 02              CPI  2          ;TEST FOR ZERO ANSWER
     634  0328 C2 2E 03           JNZ  NOT0       
