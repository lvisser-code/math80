    1229                  ;   SUBROUTINE INCR
    1230                  ;
    1231                  ;       THIS SUBROUTINE INCREMENTS THE CHARACTERISTIC
    1232                  ;       OF THE FLOATING POINT NUMBER POINTED TO BY (H,L).
    1233                  ;       WE TEST FOR OVERFLOW AND SET APPROPRIATE FLAG.
    1234                  ;       (SEE ERRROR RETURNS).
    1235                  ;
    1236                  ;   REGISTERS ON EXIT:
    1237                  ;
    1238                  ;        A = CONDITION FLAG (SEE ERROR RETURNS)
    1239                  ;        D = CLOBBERED
    1240                  ;        B,C,H,L = SAME AS ON ENTRY
    1241                  ;
    1242  053A CD C5 04   INCR:   CALL GCHAR      ;GET CHAR WITH SIGN EXTENDED
    1243  053D FE 3F              CPI  MAXCH      ;COMPARE WITH MAX CHAR PERMITTED
    1244  053F CA FA 04           JZ   OFLW1      ;INCREMENT WOULD CAUSE OVERFLOW
    1245  0542 57                 MOV  D,A        ;SAVE IT IN D
    1246  0543 14                 INR  D          ;INCREMENT IT
    1247  0544 C3 4A 05           JMP  INCR2      ;JUMP AROUND ALTERNATE ENTRY POINT
    1248  0547 2C         INCR3:  INR  L          ;COME HERE TO STORE CHARACTERISTIC
    1249  0548 2C                 INR  L          ;POINT (H,L) TO CHAR
    1250  0549 2C                 INR  L          ;POINT (H,L) TO CHAR
    1251  054A 3E 7F      INCR2:  MVI  A,177Q     
