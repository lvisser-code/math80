     616  0307 57                 MOV  D,A        ;DIFFERENCE TO D
     617  0308 3E 18      NCHK:   MVI  A,24       
     618  030A BA                 CMP  D          
     619  030B D2 10 03           JNC  SH10       
     620  030E 16 18              MVI  D,24       
     621  0310 B7         SH10:   ORA  A          
     622  0311 CD 7C 04           CALL DRST       
     623  0314 15                 DCR  D          
     624  0315 C2 10 03           JNZ  SH10       
     625  0318 7D         EQUL:   MOV  A,L        
     626  0319 B8                 CMP  B          
     627  031A C2 20 03           JNZ  EQ02       ;F.GT.S IF L.NE.B
     628  031D 69                 MOV  L,C        ;C PTR TO L
