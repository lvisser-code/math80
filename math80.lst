     457  024E 07                 RLC             
     458  024F D2 63 02           JNC  ISUBNC     ;  No, HL is positive
     459  0252 7A                 MOV  A, D       
     460  0253 07                 RLC             
     461  0254 DA 63 02           JC   ISUBNC     ;  No, DE is negative
     462  0257 7D                 MOV  A, L       ;Subtract and test sign of result
     463  0258 93                 SUB  E          
     464  0259 6F                 MOV  L, A       
