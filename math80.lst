    1022                  ;   SUBROUTINE GCHAR
    1023                  ;
    1024                  ;       THIS SUBROUTINE RETURNS THE CHARACTERISTIC OF
    1025                  ;       THE FLOATING POINT NUMBER POINTED TO BY (H,L)
    1026                  ;       IN THE A REGISTER WITH ITS SIGN EXTENDED INTO THE
    1027                  ;       LEFTMOST BIT.
    1028                  ;
    1029                  ;   REGISTERS ON EXIT:
    1030                  ;
    1031                  ;       A = CHARACTERISTIC OF (H,L) WITH SIGN EXTENDED
    1032                  ;       L = (ORIGINAL L) + 3
    1033                  ;       B,C,D,E,H = SAME AS ON ENTRY
    1034                  ;
    1035  04C5 2C         GCHAR:  INR  L          ;MAKE (H,L) POINT TO CHAR
    1036  04C6 2C                 INR  L          ;MAKE (H,L) POINT TO CHAR
    1037  04C7 2C                 INR  L          ;MAKE (H,L) POINT TO CHAR
    1038  04C8 7E                 MOV  A,M        ;SET A=CHAR + MANTISSA SIGN
    1039  04C9 E6 7F              ANI  177Q       ;GET RID OF MANTISSA SIGN BIT
    1040  04CB C6 40              ADI  100Q       ;PROPAGATE CHAR SIGN INTO LEFTMOST BIT
    1041  04CD EE 40              XRI  100Q       ;RESTORE ORIGINAL CHAR SIGN BIT
    1042  04CF C9                 RET             ;RETURN WITH (H,L) POINTING TO THE
    1043                  ;CHAR = ORIGINAL (H,L)+3
    1044                  ;SOMEONE ELSE WILL CLEAN UP
    1045                  ;
    1046                  ;
    1047                  ;   SUBROUTINE CFCHE
    1048                  ;
    1049                  ;       THIS SUBROUTINE RETURNS THE CHARACTERISTICS OF THE
    1050                  ;       FLOATING POINT NUMBERS POINTED TO BY (H,L) AND
    1051                  ;       (H,B) IN THE A AND E REGISTERS RESPECTIVELY,
    1052                  ;       WITH THEIR SIGNS EXTENDED INTO THE LEFTMOST BIT.
    1053                  ;
    1054                  ;   REGISTERS ON EXIT:
    1055                  ;
    1056                  ;       A = CHARACTERISTIC OF (H,L) WITH SIGN EXTENDED
    1057                  ;       E = CHARACTERISTIC OF (H,B) WITH SIGN EXTENDED
    1058                  ;       B,C,H,L = SAME AS ON ENTRY
    1059                  ;       D = A
    1060                  ;
    1061  04D0 5D         CFCHE:  MOV  E,L        ;SAVE LPTR IN E
    1062  04D1 68                 MOV  L,B        ;SET L = BPTR
    1063  04D2 CD C5 04           CALL GCHAR      ;GET CHAR(H,B) WITH SIGN EXTENDED IN A
    1064  04D5 6B                 MOV  L,E        ;RESTORE L = LPTR
    1065  04D6 5F                 MOV  E,A        ;SET E=CHAR(H,B) WITH SIGN EXTENDED
    1066  04D7 CD C5 04           CALL GCHAR      ;SET A=CHAR(H,L) WITH SIGN EXTENDED
    1067  04DA 2D                 DCR  L          ;RESTORE L = LPTR
    1068  04DB 2D                 DCR  L          ;RESTORE L = LPTR
    1069  04DC 2D                 DCR  L          ;RESTORE L = LPTR
    1070  04DD 57                 MOV  D,A        ;SET D=A=CHAR(H,L) WITH SIGN EXTENDED
    1071  04DE C9                 RET             
