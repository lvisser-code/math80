     605  02F8 E6 7F              ANI  127        
     606  02FA 57                 MOV  D,A        ;DIFFERENCE TO D
     607  02FB 5D                 MOV  E,L        ;SAVE BASE IN E
     608  02FC 69                 MOV  L,C        ;C PTR TO L
