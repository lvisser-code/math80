     359                  ;(HL) Print 16 bit signed integer value as ASCII number between -32768 to 32767
     360  01D1 CD DB 01   IPRINT: CALL IPSTR1     ;Convert integer value to ASCII string
     361  01D4 21 41 1F           LXI  H, STR1+1  
     362  01D7 CD 00 00           CALL MSG        ;Display the string
     363  01DA C9                 RET             
     364                  ;Convert integer value to ASCII string. Save in STR1 with format:
     365                  ;  length|string|0.
     366  01DB C5         IPSTR1: PUSH B          
     367  01DC D5                 PUSH D          
     368  01DD 01 00 00           LXI  B, 0       ;B=SIGN, C=digit counter
     369  01E0 7C                 MOV  A, H       ;If integer is negative..
     370  01E1 E6 80              ANI  80H        
     371  01E3 CA EB 01           JZ   IPSTR2     
     372  01E6 06 2D              MVI  B, '-'     ;  then SIGN = '-'
     373  01E8 CD 3E 00           CALL IABS       ;  HL = |HL|
     374  01EB 11 0A 00   IPSTR2: LXI  D, 10      
     375  01EE CD C8 00           CALL IDIV       ;(HL) ret HL / 10
     376  01F1 D5                 PUSH D          ;E = digit, stack it
     377  01F2 0C                 INR  C          ;Inc digit counter
