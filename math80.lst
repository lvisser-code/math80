    1253  054D 57                 MOV  D,A        ;BACK TO D
    1254  054E 7E                 MOV  A,M        ;NOW SIGN IT
    1255  054F E6 80              ANI  200Q       ;GET MANTISSA SIGN
    1256  0551 B2                 ORA  D          ;PUT TOGETHER
    1257  0552 77                 MOV  M,A        ;STORE IT BACK
    1258  0553 2D                 DCR  L          ;NOW BACK TO BASE
    1259  0554 2D                 DCR  L          ;/***TP
    1260  0555 2D                 DCR  L          
    1261  0556 AF         SCCFG:  XRA  A          ;SET SUCCESS FLAG
    1262  0557 C9                 RET             
