     383  01FB 71                 MOV  M, C       ;Write length
     384  01FC 78                 MOV  A, B       ;Write '-' if negative
     385  01FD B7                 ORA  A          
     386  01FE CA 07 02           JZ   IPSTR3     
     387  0201 79                 MOV  A, C       
     388  0202 3C                 INR  A          
     389  0203 77                 MOV  M, A       
     390  0204 23                 INX  H          
     391  0205 36 2D              MVI  M, '-'     
     392  0207 23         IPSTR3: INX  H          
     393  0208 D1                 POP  D          ;Unstack digit
     394  0209 7B                 MOV  A, E       
     395  020A C6 30              ADI  '0'        ;Convert to ASCII
     396  020C 77                 MOV  M, A       ;Write to STR1
     397  020D 0D                 DCR  C          
     398  020E C2 07 02           JNZ  IPSTR3     ;loop till digit counter=0
     399  0211 23                 INX  H          
     400  0212 97                 SUB  A          
