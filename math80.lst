     992                  ;       DOUBLE PRECISION SUBTRACT
     993                  ;
     994  04AA 5D         DSUB:   MOV  E,L        ;SAVE BASE IN E
     995  04AB 2C                 INR  L          ;/***TP EXTENSION
     996  04AC 2C                 INR  L          ;/START WITH LOWS
     997  04AD 7E                 MOV  A,M        ;/GET ARG
     998  04AE 68                 MOV  L,B        ;/NOW SET UP TO SUB
     999  04AF 2C                 INR  L          
    1000  04B0 2C                 INR  L          
    1001  04B1 96                 SUB  M          ;/NOW DO IT
    1002  04B2 6B                 MOV  L,E        ;/NOW MUST PUT IT BACK
    1003  04B3 2C                 INR  L          
    1004  04B4 2C                 INR  L          
    1005  04B5 77                 MOV  M,A        ;/PUT BACK
    1006  04B6 2D                 DCR  L          ;/***TP - ALL DONE
    1007  04B7 7E                 MOV  A,M        ;/GET LOW OF LOP
    1008  04B8 68                 MOV  L,B        ;/SET TO BOP
    1009  04B9 2C                 INR  L          ;/SET TO BOP LOW
    1010  04BA 9E                 SBB  M          ;/GET DIFF. OF LOWS
    1011  04BB 6B                 MOV  L,E        ;/SAVE IN LOP LOW
    1012  04BC 2C                 INR  L          ;/TO LOP LOW
    1013  04BD 77                 MOV  M,A        ;/INTO RAM
    1014  04BE 2D                 DCR  L          ;/BACK UP TO LOP HIGH
    1015  04BF 7E                 MOV  A,M        ;/GET LOP HIGH
    1016  04C0 68                 MOV  L,B        ;/SET TO BOP HIGH
    1017  04C1 9E                 SBB  M          ;/SUB. WITH CARRY
    1018  04C2 6B                 MOV  L,E        ;/SAVE IN LOP HIGH
    1019  04C3 77                 MOV  M,A        ;/INTO RAM
    1020  04C4 C9                 RET             ;/ALL DONE - MUCH SHORTER
