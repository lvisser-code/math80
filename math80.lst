    1125                  ;                             ;WE ASSUME HERE THAT ALL BYTES OF MANTISSA
    1126                  ;                             ;ARE THE SAME
    1127                  ;         CALL    WMANT       ;WRITE THE MANTISSA
    1128                  ;         MVI     A,VFLAG     ;SET ACCUMULATOR TO FLAG
    1129                  ;         ORA     A           ;SET FLAGS PROPERLY
    1130                  ;         RET                 ;RETURN (WMANT RESTORED (H,L))
    1131                  ;         ENDM
    1132                  ;
    1133                  ;WUND:   WFLT    0,100Q,377Q,UFLW1  ;WRITE UNDERFLOW
    1134  04E7 16 40      WUND:   MVI  D,100Q     ;LOAD CHARACTERISTIC INTO D REGISTER
    1135  04E9 CD 23 05           CALL WCHAR      ;WRITE CHARACTERISTIC
    1136  04EC 3E 00      UFLW1:  MVI  A,0        ;LOAD MANTISSA VALUE
    1137                  ;WE ASSUME HERE THAT ALL BYTES OF MANTISSA
    1138                  ;ARE THE SAME
    1139  04EE CD 1C 05           CALL WMANT      ;WRITE THE MANTISSA
    1140  04F1 3E FF              MVI  A,377Q     ;SET ACCUMULATOR TO FLAG
    1141  04F3 B7                 ORA  A          ;SET FLAGS PROPERLY
    1142  04F4 C9                 RET             ;RETURN (WMANT RESTORED (H,L))
    1143                  ;WOVR:   WFLT    377Q,77Q,177Q,OFLW1  ;WRITE OVERFLOW
    1144  04F5 16 3F      WOVR:   MVI  D,77Q      ;LOAD CHARACTERISTIC INTO D REGISTER
    1145  04F7 CD 23 05           CALL WCHAR      ;WRITE CHARACTERISTIC
    1146  04FA 3E FF      OFLW1:  MVI  A,377Q     ;LOAD MANTISSA VALUE
    1147                  ;WE ASSUME HERE THAT ALL BYTES OF MANTISSA
    1148                  ;ARE THE SAME
    1149  04FC CD 1C 05           CALL WMANT      ;WRITE THE MANTISSA
    1150  04FF 3E 7F              MVI  A,177Q     ;SET ACCUMULATOR TO FLAG
    1151  0501 B7                 ORA  A          ;SET FLAGS PROPERLY
    1152  0502 C9                 RET             ;RETURN (WMANT RESTORED (H,L))
    1153                  ;WIND:   WFLT    377Q,77Q,77Q,INDF1  ;WRITE INDEFINITE
    1154  0503 16 3F      WIND:   MVI  D,77Q      ;LOAD CHARACTERISTIC INTO D REGISTER
    1155  0505 CD 23 05           CALL WCHAR      ;WRITE CHARACTERISTIC
    1156  0508 3E FF      INDF1:  MVI  A,377Q     ;LOAD MANTISSA VALUE
    1157                  ;WE ASSUME HERE THAT ALL BYTES OF MANTISSA
    1158                  ;ARE THE SAME
    1159  050A CD 1C 05           CALL WMANT      ;WRITE THE MANTISSA
    1160  050D 3E 3F              MVI  A,77Q      ;SET ACCUMULATOR TO FLAG
    1161  050F B7                 ORA  A          ;SET FLAGS PROPERLY
    1162  0510 C9                 RET             ;RETURN (WMANT RESTORED (H,L))
    1163                  ;###E
    1164                  ;
    1165  0511 2C         WZER:   INR  L          ;WRITE NORMAL ZERO
    1166  0512 2C                 INR  L          ;
    1167  0513 2C                 INR  L          ;
    1168  0514 36 40              MVI  M,100Q     ;STORE CHARACTERISTIC FOR ZERO
    1169  0516 AF                 XRA  A          ;ZERO ACCUMULATOR
    1170  0517 CD 1C 05           CALL WMANT      ;STORE ZERO MANTISSA
    1171  051A B7                 ORA  A          ;SET FLAGS PROPERLY
    1172  051B C9                 RET             ;RETURN
