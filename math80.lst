     653  034A 68                 MOV  L,B        ;L
     654  034B 47                 MOV  B,A        ;AND B
     655  034C 79                 MOV  A,C        ;CPTR  TO A
     656  034D 48                 MOV  C,B        ;BPTR TO C
     657  034E 5D                 MOV  E,L        ;LPTR TO E
     658  034F 47                 MOV  B,A        ;CPTR TO B
     659  0350 CD A8 05           CALL LXFR       ;MOVE BPTR> TO LPTR>
     660  0353 78                 MOV  A,B        
     661  0354 41                 MOV  B,C        
     662  0355 4F                 MOV  C,A        
     663  0356 6B                 MOV  L,E        
