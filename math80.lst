    1447                  ;   SBUROUTINE CCHK
    1448                  ;
    1449                  ;       THIS SUBROUTINE CHECKS A CHARACTERISTIC IN
    1450                  ;       THE ACCUMULATOR FOR OVERFLOW OR UNDERFLOW.
    1451                  ;       IT THEN STORES THE CHARACTERISTIC, PRESERVING
    1452                  ;       THE PREVIOUSLY COMPUTED MANTISSA SIGN.
    1453                  ;
    1454                  ;  REGISTERS ON ENTRY:
    1455                  ;
    1456                  ;       (H,L) = ADDRESS OF ONE OPERAND
    1457                  ;       (H,B) = ADDRESS OF OTHER OPERAND
    1458                  ;       (H,C) = ADDRESS OF RESULT
    1459                  ;       A     = NEW CHARACTERISTIC OF  RESULT
    1460                  ;
    1461                  ;   REGISTERS ON EXIT:
    1462                  ;
    1463                  ;       A = CONDITION FLAG (SEE ERROR RETURNS)
    1464                  ;       D,E = GARBAGE
    1465                  ;       B,C,H,L = SAME AS ON ENTRY
    1466                  ;
    1467                  CCHK:                   ;ENTER HERE TO CHECK CHARACTERISTIC
    1468  05C8 FE 40              CPI  100Q       ;CHECK FOR 0 TO +63
    1469  05CA DA D7 05           JC   STORC      ;JUMP IF OKAY
    1470  05CD FE 80              CPI  200Q       ;CHECK FOR +64 TO +127
    1471  05CF DA DF 05           JC   OFLWC      ;JUMP IF OVERFLOW
    1472  05D2 FE C0              CPI  300Q       ;CHECK FOR -128 TO -65
    1473  05D4 DA E6 05           JC   UFLWC      ;JUMP IF UNDERFLOW
