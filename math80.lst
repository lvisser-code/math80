     300  0183 7C                 MOV  A, H       ;rotate H
     301  0184 1F                 RAR             
     302  0185 67                 MOV  H, A       
     303  0186 7D                 MOV  A, L       ;rotate L
     304  0187 1F                 RAR             
     305  0188 6F                 MOV  L, A       
     306  0189 C9                 RET             
