     197                  ;(HL, DE) 16 bit signed division: HL / DE
     198                  ;Returns HL=result, DE=remainder, CY=1 if divide by 0 error.  Uses DSIGN
     199  00C8 C5         IDIV:   PUSH B          
     200  00C9 06 00              MVI  B, 0       ;Make both args positive
     201  00CB CD 3E 00           CALL IABS       ;  while saving their signs
     202  00CE D2 D2 00           JNC  IDIVB      
     203  00D1 04                 INR  B          
     204  00D2 EB         IDIVB:  XCHG            
     205  00D3 CD 3E 00           CALL IABS       
     206  00D6 D2 DA 00           JNC  IDIVC      
     207  00D9 04                 INR  B          
     208  00DA EB         IDIVC:  XCHG            
     209  00DB 78                 MOV  A, B       
     210  00DC 32 00 00           STA  DSIGN      ;Save sign
     211  00DF 01 00 00           LXI  B, 0       ;Init quotient = 0
     212  00E2 7A                 MOV  A, D       ;Divide by 0?
     213  00E3 83                 ADD  E          
     214  00E4 C2 EB 00           JNZ  IDIV1      
     215  00E7 37                 STC             ;  CY=1
     216  00E8 C3 80 01           JMP  IDIVX      
     217  00EB CD C2 00   IDIV1:  CALL ICMP       ;Compare values
     218  00EE CA 68 01           JZ   IDIVL      ;Is divisor = dividend?
     219  00F1 DA 72 01           JC   IDIVD      ;Is divisor > dividend?
     220  00F4 7A                 MOV  A, D       ;Is devisor > 255?
     221  00F5 B7                 ORA  A          
     222  00F6 C2 68 01           JNZ  IDIVL      
     223  00F9 7B                 MOV  A, E       
     224  00FA FE 01              CPI  1          ;Is divisor = 1?
     225  00FC C2 05 01           JNZ  IDIV2      
     226  00FF 11 00 00           LXI  D, 0       ;  yes, then accelerate
     227  0102 C3 75 01           JMP  IDIVF      
     228  0105 FE 02      IDIV2:  CPI  2          ;Is divisor = 2?
     229  0107 C2 17 01           JNZ  IDIV10     
     230  010A 11 00 00           LXI  D, 0       ;  yes, then accelerate
     231  010D CD 82 01           CALL ARHL       ;  shift HL right (HL/2)
     232  0110 D2 75 01           JNC  IDIVF      
     233  0113 1C                 INR  E          
     234  0114 C3 75 01           JMP  IDIVF      
     235  0117 FE 0A      IDIV10: CPI  10         ;Is divisor = 10?
     236  0119 C2 68 01           JNZ  IDIVL      
     237  011C E5                 PUSH H          ;  yes, then accelerate
     238  011D 54                 MOV  D, H       
     239  011E 5D                 MOV  E, L       
     240  011F CD 82 01           CALL ARHL       
     241  0122 EB                 XCHG            
     242  0123 CD 82 01           CALL ARHL       
     243  0126 CD 82 01           CALL ARHL       
     244  0129 19                 DAD  D          ;  HL = dividend/2 + dividend/4
     245  012A 54                 MOV  D, H       
     246  012B 5D                 MOV  E, L       
     247  012C 0E 04              MVI  C, 4       
     248  012E CD 82 01   IDIV3:  CALL ARHL       
     249  0131 0D                 DCR  C          
     250  0132 C2 2E 01           JNZ  IDIV3      
     251  0135 19                 DAD  D          ;  HL = HL + HL/16
     252  0136 54                 MOV  D, H       
     253  0137 5D                 MOV  E, L       
     254  0138 0E 08              MVI  C, 8       
     255  013A CD 82 01   IDIV4:  CALL ARHL       
     256  013D 0D                 DCR  C          
     257  013E C2 3A 01           JNZ  IDIV4      
     258  0141 19                 DAD  D          ;  HL = HL + HL/256
     259  0142 CD 82 01           CALL ARHL       
     260  0145 CD 82 01           CALL ARHL       
     261  0148 CD 82 01           CALL ARHL       
     262  014B 44                 MOV  B, H       
     263  014C 4D                 MOV  C, L       ;  BC (result) = HL/8
     264  014D 54                 MOV  D, H       
     265  014E 5D                 MOV  E, L       
     266  014F 29                 DAD  H          
     267  0150 29                 DAD  H          
     268  0151 19                 DAD  D          
     269  0152 29                 DAD  H          
     270  0153 54                 MOV  D, H       
     271  0154 5D                 MOV  E, L       
     272  0155 E1                 POP  H          
     273  0156 CD 4D 02           CALL ISUB       
     274  0159 EB                 XCHG            ;  DE (remainder) = (dividend-HL*4 + HL)*2
     275  015A 3E 09              MVI  A, 9       ;Is remainder > 9?
     276  015C BB                 CMP  E          
     277  015D D2 73 01           JNC  IDIVE      
     278  0160 03                 INX  B          ;  Yes, then inc B & remainder -= 10
     279  0161 7B                 MOV  A, E       
     280  0162 D6 0A              SUI  10         
     281  0164 5F                 MOV  E, A       
     282  0165 C3 73 01           JMP  IDIVE      
     283                  
     284  0168 CD 4D 02   IDIVL:  CALL ISUB       ;Subtract divisor from dividend
     285  016B 03                 INX  B          ;Increment quotient
     286  016C CD C2 00           CALL ICMP       
     287  016F D2 68 01           JNC  IDIVL      
     288  0172 EB         IDIVD:  XCHG            ;Remainder in DE
     289  0173 60         IDIVE:  MOV  H, B       ;Result in HL
     290  0174 69                 MOV  L, C       
     291  0175 3A 00 00   IDIVF:  LDA  DSIGN      ;Get sign
     292  0178 1F                 RAR             ;Is sign negative?
     293  0179 D2 80 01           JNC  IDIVX      
     294  017C CD C7 01           CALL INEG       ;Quotient is negative
     295  017F 97                 SUB  A          ;CY=0
     296  0180 C1         IDIVX:  POP  B          
     297  0181 C9                 RET             
